<h2>Sign In</h2>
<%= form_with(url: "/sign_in" ,method: :post,local: true) do |f| %>
   <div><%= f.label :email %> </div>
   <div> <%= f.text_field :email %>
   </div>
   
   <div><%= f.label :password %></div>
   <div><%= f.password_field :password %> </div>

   <%= f.submit "Sign in" %>
<% end %>
<p>Don't have an account? <%= link_to "Sign Up", sign_up_path %></p>


